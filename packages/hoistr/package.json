{
  "name": "hoistr",
  "version": "0.1.0",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "typings": "dist/typings/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "!dist/**/*.tsbuildinfo",
    "!src/**/__tests__",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run dist",
    "clean": "rm -rf ./dist",
    "dist": "npm run dist:main && npm run dist:module",
    "dist:main": "../../node_modules/.bin/tsc -p ./tsconfig.build.main.json",
    "dist:module": "../../node_modules/.bin/tsc -p ./tsconfig.build.module.json"
  },
  "description": "TypeScript transformer for hoisting expressions to the module scope",
  "license": "MIT",
  "author": {
    "name": "Boris Kaul",
    "email": "localvoid@gmail.com",
    "url": "https://github.com/localvoid"
  },
  "keywords": [],
  "homepage": "https://github.com/localvoid/hoistr",
  "repository": {
    "type": "git",
    "url": "https://github.com/localvoid/hoistr"
  },
  "bugs": {
    "url": "https://github.com/localvoid/hoistr/issues"
  },
  "github": "https://github.com/localvoid/hoistr"
}